{
  "name": "email-crm-sync",
  "version": "1.0.0",
  "description": "Autonomous agent that syncs investor emails to a CRM Google Sheet",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "auth": "node scripts/auth.js",
    "backfill": "node src/backfill.js",
    "backfill:clear": "node src/backfill.js --clear",
    "backfill:10weeks": "node src/backfill.js 70 --clear",
    "fill": "node src/commands/fill-columns.js",
    "redo": "node src/commands/redo-columns.js",
    "ask": "node src/commands/ask.js",
    "help": "node src/commands/help.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.39.0",
    "googleapis": "^144.0.0",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "open": "^10.1.0"
  }
}
